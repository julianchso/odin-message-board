<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Message App</title>
  </head>
  <body>
    <h1>Mini Messaging App</h1>
    <ul>
      <% for(var i=0; i<messages.length; i++) { %>
        <li>
            <%= messages[i].user %>
            <%= messages[i].text %>
            <%= messages[i].added %>
            <%= messages[i].id %>
        </li>
    <% } %>
    </ul>
  </body>
</html>
;
